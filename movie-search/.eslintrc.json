{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "no-undef": 0,
    "import/no-cycle": 0
  }
}
